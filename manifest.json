  {
    "name": "Bookmark Locker",
    "version": "1.0",
    "description": "manage your bookmarks and folders, lock and hide them!",
    "manifest_version": 2,
    "permissions": [
        "bookmarks", "storage", "tabs", "unlimitedStorage"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent":false
    },
    "browser_action": {
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
        }
    },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      }
  }